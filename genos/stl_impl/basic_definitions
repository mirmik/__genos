

#ifndef GSTD__BASIC_DEFINITIONS
#define GSTD__BASIC_DEFINITIONS 1

#include <genos/stl_impl/system_configuration.h>

#pragma GCC visibility push(default)

#ifdef GCC_HASCLASSVISIBILITY
	#define _UCXXEXPORT __attribute__ ((visibility("default")))
	#define _UCXXLOCAL __attribute__ ((visibility("hidden")))
#else
	#define _UCXXEXPORT
	#define _UCXXLOCAL
#endif

#pragma GCC visibility pop

#endif